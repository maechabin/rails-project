<h1>Users#new</h1>
<p>Find me in app/views/users/new.html.erb</p>

<%= form_for User.new do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <%= f.label :location %>
  <%= f.text_field :location %>
  <%= f.label :about %>
  <%= f.text_area :about %>
  <%= f.submit %>
<% end %>
